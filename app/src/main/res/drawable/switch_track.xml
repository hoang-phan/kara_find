<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    android:visible="true"
    android:dither="true"
    android:useLevel="false">
    <gradient
        android:startColor="@color/track_dark"
        android:endColor="@color/track_light"
        android:angle="270"/>
    <corners
        android:radius="@dimen/track_radius"/>
    <size
        android:width="@dimen/track_width"
        android:height="@dimen/track_height" />
</shape>